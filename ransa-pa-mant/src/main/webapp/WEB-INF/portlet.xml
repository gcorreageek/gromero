		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports> 	
		<portlet-info>
			<title>Mantenimiento Primax EmpresaAreaTD</title>
			<short-title>mantPrimaxEmpresaAreaTD</short-title>
			<keywords>mant, mantenimiento, primax</keywords>
		</portlet-info>
		<portlet-preferences>
            <preference><name>registrosPagina</name><value>10</value></preference>
         </portlet-preferences>
	</portlet>
	<portlet>
  		<description>MantPrimaxAccesoUsuarioPortlet</description>
		<portlet-name>MantPrimaxAccesoUsuarioPortlet</portlet-name>
	    <display-name>MantPrimaxAccesoUsuarioPortlet</display-name>
		<portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
		<init-param>
